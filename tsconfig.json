{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "allowSyntheticDefaultImports": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "es5",
    "jsx": "react",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "traceResolution": true,
    "lib": ["es5", "es6", "dom"],
  },
  "include": [
    "./src/**/*"
  ]
}
